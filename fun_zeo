function ka = fun_zero(x,K,k,sin,sout)
ka = [prod(x,'all') - sout/sin;
    k(1)*(1-x(1)/K(1))-k(2)*(1-x(2)/K(2));
    k(2)*(1-x(2)/K(2))-k(3)*(1-x(3)/K(3));
    k(3)*(1-x(3)/K(3))-k(4)*(1-x(4)/K(4));
    k(4)*(1-x(4)/K(4))-k(5)*(1-x(5)/K(5));
    k(5)*(1-x(5)/K(5))-k(6)*(1-x(6)/K(6));
    k(6)*(1-x(6)/K(6))-k(7)*(1-x(7)/K(7));
    k(7)*(1-x(7)/K(7))-k(8)*(1-x(8)/K(8));
    k(8)*(1-x(8)/K(8))-k(9)*(1-x(9)/K(9));
    k(9)*(1-x(9)/K(9))-k(10)*(1-x(10)/K(10));
    ];
end
